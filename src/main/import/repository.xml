<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <moderate-messages j:dependencies="default siteSettings tabularList"
                           j:modulePriority="0"
                           j:moduleType="module"
                           j:title="Moderate Messages"
                           jcr:mixinTypes="jmix:hasExternalProviderExtension"
                           jcr:primaryType="jnt:module">

            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <message_ok.png j:height="256"
                               j:width="256"
                               jcr:mixinTypes="jmix:image"
                               jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </message_ok.png>
                        <message_ok-resize16x16.png j:height="16"
                                           j:width="16"
                                           jcr:mixinTypes="jmix:image"
                                           jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </message_ok-resize16x16.png>
                    </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <site-settings-base j:view="moderateMessages"
                                j:workInProgress="false"
                                    jcr:mixinTypes="jmix:useUILocale"
                                    jcr:primaryType="jnt:template">
                        <moderate-messages j:applyOn="jnt:virtualsite"
                                      j:defaultTemplate="false"
                                      j:hiddenTemplate="false"
                                 j:icon="/modules/moderate-messages/templates/files/message_ok-resize16x16.png"
                                  j:requiredPermissionNames="adminSiteSettingsModerateMessages"
                                 jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <mod-messages-tabbed-list j:view="moderateMessages"
                                               jcr:mixinTypes="jmix:renderable"
                                               jcr:primaryType="jnt:tabularList">
                                    <conversations-moderation-tab jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Moderation Panel"/>
                                        <j:translation_fr jcr:language="fr"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Moderer les derniers messages"/>
                                        <conversations-moderation-panel jcr:primaryType="jnt:moderateMessages">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"/>
                                        </conversations-moderation-panel>
                                    </conversations-moderation-tab>
                                    <validated-conversations-tab jcr:primaryType="jnt:contentList">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="All Validated Conversations"/>
                                        <j:translation_fr jcr:language="fr"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Voir tous les messages validÃ©s"/>
                                        <validated-conversations-panel jcr:primaryType="jnt:viewAllMessages">
                                            <j:translation_en j:workInProgress="false"
                                                jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"/>
                                        </validated-conversations-panel>
                                    </validated-conversations-tab>
                                </mod-messages-tabbed-list>
                            </pagecontent>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Moderate Messages"/>
                        </moderate-messages>
                </site-settings-base>
            </templates>

        </moderate-messages>
    </modules>
</content>
